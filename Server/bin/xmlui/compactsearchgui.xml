<mc2gtk>

<window title="Search" id="top_level_window" >

<vbox fill="false" expand="false" padding="0">
  <frame label="Server and user settings">
    <include file="xmlui/serversettingsbox.xml" />
  </frame>
  <hbox>
    <label text="language:" />
    <entry id="language_replace" value="swedish" />
  </hbox>
  <hbox>
    <label text="start:" />
    <entry id="start_replace" value="0" />
  </hbox>
  <hbox>
    <label text="end:" />
    <entry id="end_replace" value="3" />
  </hbox>
  <hbox>
    <label text="max hits:" />
    <entry id="max_hits_replace" value="30" />
  </hbox>
  <hbox>
    <label text="heading:" />
    <entry id="heading_replace" value="-1" />
  </hbox>
  <hbox>
    <label text="round:" />
    <spinbutton id="round_replace" min="0" max="1" step="1" />
  </hbox>
  <hbox>
    <label text="top region:" />
    <entry id="top_region_replace" value="1" />
  </hbox>
  <hbox>
    <label text="category search:" />
    <entry id="category_search_replace" value="false" />
  </hbox>
  <frame label="Search fields: " >
   <vbox>
    <hbox>
      <hbox>
         <label text="what:" />
         <entry id="what_replace" value="wi-fi" />
      </hbox>
      <hbox>
         <label text="where:" />
         <entry id="where_replace" value="lund" />
      </hbox>
      <hbox>
         <label text="category:" />
         <entry id="category_replace" value="" />
      </hbox>
      
    </hbox>
    <hbox>
         <label text="lat/lon" />
         <entry id="lat_replace" value="664748447" />
         <entry id="lon_replace" value="157366099" />
         <label text="angle:" />
         <entry id="angle_replace" value="" />
    </hbox>
   </vbox>
  </frame>

  <button id="send_button" label="send!" />
  <frame id="result_window" label="Search results">
  </frame>

</vbox>
</window>

</mc2gtk>
